{
    "name": "meng-tian/soap-http-binding",
    "description": "A PHP library for binding SOAP messages to PSR-7 HTTP messages.",
    "keywords": ["SOAP", "HTTP", "PSR-7"],
    "license": "MIT",
    "authors": [
        {
            "name": "Meng Tian",
            "email": "tianmeng94@hotmail.com"
        },
        {
            "name": "Patrique Ouimet",
            "email": "patrique.ouimet@gmail.com"
        }
    ],
    "repositories": [
        {
            "url": "git@github.com:patoui/php-soap-interpreter.git",
            "type": "git"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-soap": "*",
        "zendframework/zend-diactoros": "~1.3",
        "meng-tian/php-soap-interpreter": "dev-master"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "9.*"
    },
    "autoload": {
        "psr-4": {
            "Meng\\Soap\\HttpBinding\\": "src/",
            "Tests\\": "tests/"
        }
    }
}
